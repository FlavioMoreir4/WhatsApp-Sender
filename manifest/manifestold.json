   "permissions": [ "*://*.whatsapp.com/*", "*://*/*", "activeTab", "background","system.storage","debugger","downloads" ,"contentSettings", "clipboardRead","clipboardWrite","clipboardWrite","declarativeNetRequest","declarativeNetRequest","nativeMessaging","pageCapture","pageCapture","pageCapture","tabs"],
   "short_name": "WhatsBulk",
